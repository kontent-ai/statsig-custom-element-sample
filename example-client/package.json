{
  "name": "@kontent-ai/statsig-experiment-example",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "generate:types": "dotenv -e .env -- sh -c 'kontent-generate delivery-sdk --environmentId=$VITE_KONTENT_ENVIRONMENT_ID --managementApiKey=$KONTENT_MANAGEMENT_API_KEY --outputDir=src/models'",
    "sync:types": "tsx scripts/sync-types.ts",
    "sync:content": "tsx scripts/sync-content.ts",
    "sync:all": "pnpm sync:types && pnpm sync:content"
  },
  "dependencies": {
    "@kontent-ai/delivery-sdk": "16.4.0",
    "@kontent-ai/rich-text-resolver": "^3.0.2",
    "@kontent-ai/rich-text-resolver-react": "3.0.2",
    "@statsig/react-bindings": "^3.30.2",
    "@tanstack/react-query": "5.90.12",
    "react": "^19.2.1",
    "react-dom": "^19.2.1"
  },
  "devDependencies": {
    "@kontent-ai/data-ops": "latest",
    "jszip": "latest",
    "@kontent-ai/model-generator": "^9.1.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "dotenv": "17.2.3",
    "dotenv-cli": "^11.0.0",
    "tsx": "4.21.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.7"
  }
}
